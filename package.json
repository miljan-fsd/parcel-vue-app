{
  "name": "parcel-vue-app",
  "version": "1.0.0",
  "description": "Parcel Vue App",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development parcel ./index.html -p 3000 --open",
    "build":
      "cross-env NODE_ENV=production parcel build ./index.html -d build --public-url ./"
  },
  "keywords": ["Vue", "Parcel"],
  "author": "Miljan Djordjevic <miljan.fsd@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bulma": "^0.7.1",
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^1.1.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-vue": "^4.5.0",
    "parcel-bundler": "^1.7.1",
    "parcel-plugin-eslint": "^1.0.4",
    "parcel-plugin-vue": "^1.5.0",
    "prettier": "1.12.1",
    "vue-template-compiler": "^2.5.16"
  }
}
